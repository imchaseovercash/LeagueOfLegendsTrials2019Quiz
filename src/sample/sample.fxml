<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane fx:id="mainPane" alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <BorderPane prefHeight="800.0" prefWidth="1280.0">
         <top>
            <Label id="startLabel" alignment="CENTER" prefHeight="207.0" prefWidth="1280.0" text="Would You Like to Take the League of Legends 2019 Trials Quiz?" textAlignment="CENTER" wrapText="true" BorderPane.alignment="CENTER">
               <font>
                  <Font size="48.0" />
               </font>
            </Label>
         </top>
         <center>
            <Button id="startButton" onAction="#HandleStartButton" text="Take Quiz!" BorderPane.alignment="CENTER">
               <font>
                  <Font size="36.0" />
               </font>
            </Button>
         </center></BorderPane>
   </children>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
</GridPane>
